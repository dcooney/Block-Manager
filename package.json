{
	"name": "block-manager",
	"version": "1.0.0",
	"description": "Manage all your Gutenberg blocks",
	"main": "src/js/app.js",
	"scripts": {
		"start": "webpack -w --mode development --config=webpack/dev.config.js",
		"build": "webpack -p --mode production --config=webpack/prod.config.js",
		"release": "webpack -p --mode production --config=webpack/prod.config.js && npm run clean && npm run pot",
		"clean": "cd ./dist/ && del-cli js/gbm-admin.js.map js/gbm.js.map js/style.js js/style.js.map css/style.css.map",
		"pot": "composer run pot"
	},
	"keywords": [],
	"author": "ConnektMedia",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.21.4",
		"classnames": "^2.3.1",
		"es6-promise": "^4.1.1",
		"imagesloaded": "^4.1.3",
		"isomorphic-fetch": "^2.2.1",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"resolve-url-loader": "^2.1.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-optional-chaining": "^7.16.0",
		"autoprefixer": "^7.1.4",
		"babel-core": "^6.25.0",
		"babel-loader": "^7.1.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"copy-webpack-plugin": "^4.5.0",
		"css-loader": "^0.28.7",
		"del-cli": "^5.0.0",
		"extract-text-webpack-plugin": "4.0.0-alpha.0",
		"file-loader": "^2.0.0",
		"jshint": "^2.13.1",
		"node-sass": "^6.0",
		"postcss-import": "^10.0.0",
		"postcss-loader": "^2.0.6",
		"sass-loader": "^10.4.1",
		"style-loader": "^0.18.2",
		"url-loader": "^1.1.2",
		"webpack": "^4.46.0",
		"webpack-cli": "^3.3.12"
	}
}
